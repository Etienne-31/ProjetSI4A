  }

//if else marche 
int main() {
  int x = 4672;
  x = 33+49+x;
  if (x){
      int r;
      r=3980;
    }else{
      
      x=900000;
    }
  }

//test ajout de fonctions
int compute2(int a,int c) {
  int b = 309;
  a = 23903/3930;
  return a;
} 

int compute(int a, int b) {
  int c;
  c = 33+49+c;
  return c;
} 

