//test while 
int main(){
  int x = 0;
  int y=8888;
  while(x<y){
    x=x+34;
  }
  x=12;
   while(x){
    x=x+444444;
  }
  x=5555555;
}

//test while imbriqués 
int main(){
  int x;
  int y;
  while(x==y){
    x=x+34;
    while(x){
    x=x+444444;
  }
  x=333333;
  }
  x=12;
   
  x=5555555;
}


//operations arithmetiques
int main() {
  int x = 309;
  int y;
  x = 33+49+x+233;
  y= x-409409;
  y = 23903/3930;
  x = 309039*10192;

  x = 33+49+x;
  y= x-409409;
  y = 23903/3930;
  x = 309039*10192;
} 
//if imbriqués ok 
int main() {
  int y = 4672;
