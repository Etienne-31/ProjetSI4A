//test ajout de fonctions

int compute(int a, int b) {
  a = a+18;
  if (a<60){
    a=390;
  }else{
    if (a==70){
      a=209;
    }else{
      a=3333;
    }
  }

  return a;
} 
int compute2(int a) {
  int c;
  c=20;
  a=90+c+a;
  return a;
} 

int main(){
    int y ;
    int x ;
    int z;
    int b;
    int e;
    x= compute(32,x);
    y = compute2(12);
    e = compute(53,x);
    z= compute2(10);
    b = compute(52,x);
    print(b); //209
    print(x);//390
    print(y);//492
    print(z);//120
    print(e);//3333

  }

