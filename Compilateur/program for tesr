//test calcul 
  int main(){
    int x; 
    int y=10,z;
    z = 12;
    x=z+y;
    z=x;
    while(z>19){
      if(y==10){
        y=y-1;
        z = z-1;
      }else{
        z=z-2;
      }
      print(z);
    }
    print(z);//19
    print(x);//22
    print(y);//9
  }
//main simple
int comp(int a, int b){
  a = a+b;
  return a;
}

int compute(int a){
  int c =2;
  a= a+c+2;//30
  a= comp(a,12);//42
  return a;
}

int compute2(int a){
  int c = 10;
  c=compute(a);//26
  c = compute(c);//42
  a=a+c;//52
  return a;
}
int main(){
  int x,y,z,b,e;
  y = compute2(10);//52
  x = compute(20);//3
  print(y);//52
  print(x);//36
}

//test slides cours 
int f(int a) { 
  return 2;
}
int g() {
  int a = f(2);
}
int h() {
  int a = g();
}
//appel fonction complexe

int compute(int a){
  int c =2;
  a= a+c+2;//30
  return a;
}
int comp(int a, int b){
  int c=2;
  if (a+c==b+c){
    if (a<5){
      a= compute(a);//6
      print(a);
    }
  }else {
    a = compute(b);//8
    print(a);
  }
  return a;
}



int compute2(int a){
  int c = 10;
  int d,f;
  c=compute(a);//4
  print(c);
  d=comp(2,2);//6
  print(d);
  f=comp(3,4);
  print(f);//3
  a=a+c;//52
  return a;
}
int main(){
  int x,y,z,b,e;
  y = compute2(10);//52
  x = compute(20);//3
  print(y);//28
  print(x);//24
}
//kehdoned
int compute(int a){
  a=a+1;
  return a;

}
int comp(int a, int b){
  int c=2;
  if (a==b){
      a= compute(a);//6
  }else {
    a = compute(b);//8
  }
  return a;
}

int main(){
  int y;
  y = comp(12,13);//52
  print(y);//28

}

//main
int main(int a, int b){
  int c=2;
  if (a==b){
    if (a<5){
      a= a+2;//6
    }
  }else {
    a = a+3;//8
  }
}


